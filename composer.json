{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "egulias/email-validator": "^2.1",
        "friendsofsymfony/rest-bundle": "^2.7",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
        "jms/serializer": "^3.4",
        "jms/serializer-bundle": "^3.5",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nelmio/api-doc-bundle": "^3.5",
        "nelmio/cors-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/orm-pack": "^1.0",
        "symfony/property-info": "4.*",
        "symfony/security-bundle": "4.*",
        "symfony/test-pack": "^1.0",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "thecodingmachine/safe": "^1.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^7.0",
        "fzaninotto/faker": "^1.9",
        "phpstan/phpstan": "^0.12.14",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/maker-bundle": "^1.14",
        "symfony/monolog-bundle": "^3.5",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "4.4.*",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "thecodingmachine/phpstan-strict-rules": "^0.12.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "csfix": "phpcbf --ignore=src/Migrations/**,src/Kernel.php",
        "cscheck": "phpcs --ignore=src/Migrations/**,src/Kernel.php",
        "phpstan": "phpstan analyse src/ -c phpstan.neon --level=7 --no-progress -vvv --memory-limit=1024M"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
