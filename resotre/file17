import Vue from 'vue';
import ElementUI from 'element-ui';
import Vuetify from "./Plugins/Vuetify";
import i18n from "./Plugins/I18n";
import {AppModule} from "./Modules/Common/Store/AppModule";
import router from "./Guard.ts";
import App from './App.vue';
import Store from "./Store.ts";

Vue.config.productionTip = false;
Vue.use(ElementUI, {
    size: AppModule.size,
    i18n: (key: string, value: string) => i18n.t(key, value)
})

const app =  new Vue({
    el: '#app',
    i18n: i18n,
    router: router,
    vuetify: Vuetify,
    store: Store,
    render: h => h(App),
});

export default app;