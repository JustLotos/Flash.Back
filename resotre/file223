import Vue from 'vue';

declare module '*.vue' {
  import Vue from 'vue'
  export default Vue
}

declare module 'vuetify/lib' {
  import 'vuetify/types/lib'
}

declare module 'vue-router' {
  interface Next<T extends Vue = Vue> {
    /* eslint-disable @typescript-eslint/no-explicit-any */
    (to?: (vm: T) => any): void;
  }
}
