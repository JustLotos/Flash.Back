<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <title>FlashBack</title>
    {% block stylesheets %}
        <link rel="shortcut icon" href="{{ asset('build/flash.svg') }}" type="image/x-icon">
        <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900" rel="stylesheet">
        <link href="https://cdn.jsdelivr.net/npm/@mdi/font@4.x/css/materialdesignicons.min.css" rel="stylesheet">
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
    {% block content %}
        <div id="app"></div>
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</body>
    <script>
        localStorage.setItem('routes',JSON.stringify({{ routes|json_encode(constant('JSON_UNESCAPED_UNICODE'))|raw }}))
    </script>
</html>